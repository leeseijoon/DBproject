		<form role="form">
			<fieldset>
				<h2>Buy BTC</h2>
				<div class="form-group">
                   Amount(BTC) <input type="number" name="AmountBTC" id="AmountBTC1" class="form-control input-lg">
				</div>
				<div class="form-group">
                   Price(KRW) <input type="number" name="AmountKW" id="AmountKW1" class="form-control input-lg" >
				</div>
				<div class="form-group">
                   Total KRW <input type="number" name="AmountKW" id="TotalKW1" class="form-control input-lg" readonly>
				</div>
				<div class="row">
					<div class="col-xs-6 col-sm-6 col-md-6">
                        <input type="submit" class="btn btn-lg btn-success btn-block" value="Buy">
					</div>
				</div>
			</fieldset>
		</form>
	</div>
</div>


<div class="row" style="margin-top:20px">
    <div class="col-xs-12 col-sm-8 col-md-6 col-sm-offset-2 col-md-offset-3">
